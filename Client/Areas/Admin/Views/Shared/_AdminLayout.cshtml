    <script src="~/Assets/Admin/Home/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/Assets/Admin/Home/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/Assets/Admin/Home/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/Assets/Admin/Home/js/main.js"></script>
    
    @await RenderSectionAsync("Scripts", required:false)

</body>

</html>